package week1.day1;



public class PrimeNumber {


	public static void main(String[] args) {

		int i=13;

		boolean flag= false;
		
			for (int j=2;j<=i;j++)
			{
				if (j%1==0 && j%j==0)
				{
					flag=true;
				}
				break;
			}
		if (flag == true)
		{
			System.out.println (i + " is prime");
		}
		else
		{
			System.out.println(i+" is not prime");
		}
	
		
		
